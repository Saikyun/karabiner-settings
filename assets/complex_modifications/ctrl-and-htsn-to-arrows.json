{
    "title": "Change right_ctrl",
    "rules": [
        {
            "description": "c+c-a/e -> c-home/end",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "home",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "end",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "c-a/e -> home/end",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "home"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "end"
                        }
                    ]
                }
            ]
        },
        {
            "description": "c-g/c -> pgdn/up",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_down"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_up"
                        }
                    ]
                }
            ]
        },
        {
            "description": "c-å/ä -> o-backspace/delete",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace",
                            "modifiers": [
                                "right_option"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward",
                            "modifiers": [
                                "right_option"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "caps -> backspace => right_ctrl",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "caps_lock",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_control"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ]
                }
            ]
        },
        {
            "description": "r-c u -> return",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "r-c r / l -> forward/backward word",
            "manipulators": [
                {
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "right_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "right_option"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "r-c htns -> arrow keys",
            "manipulators": [
                {
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
